<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="TestNG Test Suite - 2">
    <test name="Customer Search">
        <classes>
            <class name="annotations.ConfigurationAnnotations">
                <methods>
                    <include name="searchCustomer"/>
                </methods>
            </class>
        </classes>
    </test>  <!-- End of Test "Customer Search" -->

    <test name="Product Search">
        <classes>
            <class name="annotations.ConfigurationAnnotations">
                <methods>
                    <include name="searchProduct"/>
                </methods>
            </class>
        </classes>
    </test>  <!-- End of Test "Product Search" -->
</suite>  <!-- End of Suite "TestNG Test Suite - 2" -->
