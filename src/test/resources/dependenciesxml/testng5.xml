<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="DependsOnGroups Test Suite - 5">
    <test name="Include Regression - Exclude Defect Test Group">
        <groups>
            <run>
                <include name="regression"/>
                <exclude name="defect.*"/>
            </run>
        </groups>
        <classes>
            <class name="dependencies.GroupsTests"/>
        </classes>
    </test>
</suite>